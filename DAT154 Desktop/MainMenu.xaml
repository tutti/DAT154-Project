<Page x:Class="DAT154_Desktop.MainMenu"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DAT154_Desktop"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="MainMenu">

    <StackPanel>
        <Label HorizontalAlignment="Center" Content="Main menu"></Label>
        <Button Margin="10,10" Name="bookingButton" Content="Booking"></Button>
        <Button Margin="10,10" Name="serviceButton" Content="Service"></Button>
        <Button VerticalAlignment="Bottom" Margin="10,10" Name="logoutButton" Content="Log out"></Button>
    </StackPanel>
</Page>
